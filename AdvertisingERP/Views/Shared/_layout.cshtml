
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Advertising ERP</title>
    <!-- loader-->
    @*<link href="~/assets/css/pace.min.css" rel="stylesheet" />
        <script src="~/assets/js/pace.min.js"></script>*@
   
    <!--favicon-->
    <script src="~/assets/js/pace.min.js"></script>
    @*<link rel="icon" href="~/assets/images/favicon.ico" type="image/x-icon">*@
    <!-- Vector CSS -->
    <link href="~/assets/plugins/vectormap/jquery-jvectormap-2.0.2.html" rel="stylesheet" />
    <!-- simplebar CSS-->
    <link href="~/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <!-- Bootstrap core CSS-->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS-->
    <link href="~/assets/css/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="~/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Sidebar CSS-->
    <link href="~/assets/css/sidebar-menu.css" rel="stylesheet" />
    <!-- Custom Style-->
    <link href="~/assets/css/app-style.css" rel="stylesheet" />

    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/sidebar-menu.js"></script>
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/app-script.js"></script>
    <script src="~/assets/js/index.js"></script>
    <link href="~/assets/css/style.css" rel="stylesheet" />
</head>
<body class="bg-theme bg-theme1">

    <!-- Start wrapper-->
    <div id="wrapper">

        <!--Start sidebar-wrapper-->
        <div id="sidebar-wrapper" data-simplebar="" data-simplebar-auto-hide="true">
            <div class="brand-logo">
                <a href="index.html">
                    <img src="~/assets/images/footer_logo.png" class="logo-icon" alt="logo icon" />
                    @*<img src="~/assets/images/logo-icon.png" class="logo-icon" alt="logo icon">*@
                    @*<h5 class="logo-text">Admin</h5>*@
                </a>
            </div>
            <ul class="sidebar-menu do-nicescrol">
                <li class="sidebar-header">MAIN NAVIGATION</li>
                <!-- Dropdown-->
                <li>
                    <a href="#">
                        <i class="zmdi zmdi-view-dashboard"></i> <span>Dashboard</span>
                    </a>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown-dashboard">
                        <i class="zmdi zmdi-collection-item"></i><span>Masters</span>
                        @*<span class="caret"></span>*@
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown-dashboard" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"><a href="/Master/SiteMaster"> <i class="zmdi zmdi-cast-connected"></i> Site Master</a></li>
                                <li class="dropdown-item"><a href="/Master/Servicemaster"> <i class="zmdi zmdi-widgets"></i> Service Master</a></li>
                                <li class="dropdown-item"><a href="/Master/FinancialYear"> <i class="zmdi zmdi-money-box"></i> Financial Year</a></li>
                                <li class="dropdown-item"><a href="/Master/BankMaster"> <i class="zmdi zmdi-home"></i> Bank Master</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown_Customer">
                        <i class="zmdi zmdi-account-circle"></i><span>Customer</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown_Customer" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"> <i class="fa fa-user-plus mr-2"></i> <a href="/Customer/AddCustomer">Add Customer</a></li>
                                <li class="dropdown-item"> <i class="fa fa-list-ul mr-2"></i><a href="/Customer/CustomerList"> Customer List</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown_Vendor">
                        <i class="zmdi zmdi-accounts"></i><span>Vendor</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown_Vendor" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"> <i class="zmdi zmdi-view-dashboard"></i> <a href="/Vendor/AddVendor"> Add Vendor</a></li>
                                <li class="dropdown-item"> <i class="zmdi zmdi-view-dashboard"></i> <a href="/Vendor/VendorList"> Vendor List</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown-Campaign">
                        <i class="zmdi zmdi-memory"></i><span>Campaign</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown-Campaign" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"> <i class="zmdi zmdi-view-dashboard"></i><a href="/Compaign/CampaignMaster"> Add Campaign</a></li>
                                <li class="dropdown-item"> <i class="zmdi zmdi-view-dashboard"></i><a href="/Compaign/CompaignList"> Campaign List</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown-Sale">
                        <i class="zmdi zmdi-shopping-cart"></i><span>Sale Order</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown-Sale" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"><a href="/SaleOrder/GenerateSaleOrder"> <i class="zmdi zmdi-file-add"></i> Generate Sale Order</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown-Purchase">
                        <i class="zmdi zmdi-shopping-basket"></i><span>Purchase Order</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown-Purchase" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"><a href="/PurchaseOrder/POList"> <i class="zmdi zmdi-file-add"></i> Generate PO</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li class="panel panel-default" id="dropdown">
                    <a data-toggle="collapse" href="#dropdown-Invoice">
                        <i class="zmdi zmdi-assignment-check"></i><span>Invoice</span>
                        <i class="fa fa-plus caret1"></i>
                    </a>
                    <!-- Dropdown dashboard 1 -->
                    <div id="dropdown-Invoice" class="panel-collapse collapse">
                        <div class="panel-body">
                            <ul class="nav navbar-nav sub-menu">
                                <li class="dropdown-item"><a href="/SaleOrder/InvoiceNo"> <i class="zmdi zmdi-assignment-o"></i> Generate Invoice</a></li>
                            </ul>
                        </div>
                    </div>
                </li>
                <li>
                    <a href="/Home/Login">
                        <i class="icon-power mr-2"></i> <span>Logout</span>
                    </a>
                </li>
            </ul>
        </div>
        <!--End sidebar-wrapper-->
        <!--Start topbar header-->
        <header class="topbar-nav">
            <nav class="navbar navbar-expand fixed-top">
                <ul class="navbar-nav mr-auto align-items-center">
                    <li class="nav-item">
                        <a class="nav-link toggle-menu" href="javascript:void();">
                            <i class="icon-menu menu-icon"></i>
                        </a>
                    </li>
                </ul>
                <ul class="navbar-nav align-items-center right-nav-link">
                    <li class="nav-item dropdown-lg">
                        <a class="nav-link dropdown-toggle dropdown-toggle-nocaret waves-effect" data-toggle="dropdown" href="javascript:void();">
                            <i class="fa fa-bell-o"></i>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link dropdown-toggle dropdown-toggle-nocaret" data-toggle="dropdown" href="#">
                            <span class="user-profile"><p class="new"><i class="fa fa-user-o"></i></p></span>
                        </a>
                        <ul class="dropdown-menu dropdown-menu-right">
                            <li class="dropdown-item user-details">
                                <a href="javaScript:void();">
                                    <div class="media">
                                        <div>
                                            <i class="fa fa-user-o mr-2"></i>
                                        </div>
                                        <div class="media-body">
                                            <h6 class="user-title">@Session["Username"]</h6>
                                            <p class="user-subtitle">@Session["EmailId"]</p>
                                        </div>
                                    </div>
                                </a>
                            </li>
                           
                            <li class="dropdown-item"><i class="icon-power mr-2"></i> <a href="/Home/Login">Logout</a></li>
                        </ul>
                    </li>
                </ul>
            </nav>
        </header>
        <!--End topbar header-->
        <div class="clearfix"></div>
                @RenderBody()
                <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>

                <!--End Back To Top Button-->
                <!--Start footer-->
                <footer class="footer">
                    <div class="container">
                        <div class="text-center">
                            Copyright © 2022 Afluex Multiservices LLP
                        </div>
                    </div>
                </footer>
                <!--End footer-->
                <!--start color switcher-->
                <!--end color switcher-->

            </div><!--End wrapper-->
            <!-- Bootstrap core JavaScript-->
            <script src="~/assets/js/jquery.min.js"></script>
            <script src="~/js/popper.min.js"></script>
            <script src="~/js/bootstrap.min.js"></script>

            <!-- simplebar js -->
            <script src="~/plugins/simplebar/js/simplebar.js"></script>
            <!-- sidebar-menu js -->
            <script src="~/js/sidebar-menu.js"></script>
            <!-- loader scripts -->
            <script src="~/js/jquery.loading-indicator.html"></script>
            <!-- Custom scripts -->
            <script src="~/js/app-script.js"></script>
            <!-- Chart js -->

            <script src="~/plugins/Chart.js/Chart.min.js"></script>

            <!-- Index js -->
            <script src="~/assets/js/index.js"></script>
</body>

</html>

